//check if array contains a number
public class numSearch {
public static void main(String args[]){
	int a[]={1,8,3,8,12,8};
	int ele=8;
	System.out.println("\nIn the array, -> ");
	for(int i=0;i<6;i++)
		System.out.print(a[i]+" ");
	System.out.println();
	for(int i=0;i<6;i++)
	{
		if(ele==a[i])
		{
			System.out.println("Element "+ele+" found at "+(i+1));
		}
	}
}
}
