//find missing number in array of (n-1) length
public class missingNumber {
	public static void main(String args[]){
		int a[]={1,2,3,5,6,7,8,9,10},sum=0,sum1=0,res=0;
		System.out.println("The array is: ");
		for(int i=0;i<9;i++)
			System.out.print(a[i]+" ");
		for(int i=0;i<9;i++)
			sum+=a[i];
		for(int i=1;i<=10;i++)
			sum1+=i;
		res=sum1-sum;
		System.out.println("\nThe missing element is: "+res);
		
	}

}
